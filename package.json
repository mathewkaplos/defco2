{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:e2e:ci": "cross-env NODE_ENV=test dotenv -e .env.test -- playwright test --config=playwright.config.ts",
    "test:e2e:ui": "cross-env NODE_ENV=test dotenv -e .env.test -- playwright test --config=playwright.config.ts --ui",
    "test:e2e": "npm run test:e2e:ui",
    "test:backend": "dotenv -e .env.test -- jest -i --testMatch=\"**/*.test.ts\" --silent",
    "test:backend:verbose": "dotenv -e .env.test -- jest --testMatch=\"**/*.test.ts\" -i ",
    "postinstall": "prisma generate",
    "start:test": "cross-env NODE_ENV=test npm run build && cross-env NODE_ENV=test npm run start",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:setup:script": "ts-node -T ./src/prisma/prismaSetupScript.ts",
    "prisma:migrate:development": "npx prisma generate && dotenv -e .env.local -- prisma migrate dev && dotenv -e .env.local -- npm run prisma:setup:script",
    "prisma:migrate:production": "prisma migrate deploy && npm run prisma:setup:script",
    "setup": "npm install && npm run prisma:migrate:development",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/subscription/webhook"
  },
